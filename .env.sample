# Sample .env file for Telegram Bot Monitor

# Telegram API credentials
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here
TELEGRAM_PHONE_NUMBER=+1234567890  # Your phone number in international format

# Target bot username (include @)
TARGET_BOT=@target_bot_username

# Comma-separated list of user IDs to receive alerts
ALERT_USER_IDS=123456789,987654321  # Replace with actual user IDs 